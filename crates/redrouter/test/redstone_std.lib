library(redstone_std) {

  cell(BUF) {
    area: 1;
    pin(A) {
      direction: input;
    }
    pin(Y) {
      direction: output;
      function: "A";
    }
  }

  cell(NOT) {
    area: 9
    pin(A) {
      direction: input;
    }
    pin(Y) {
      function: "A'";
      direction: output;
    }
  }

  cell(OR) {
    area: 2
    pin(A) {
      direction: input;
    }
    pin(B) {
      direction: input;
    }
    pin(Y) {
      function: "(A|B)";
      direction: output;
    }
  }

  cell(DFF) {
    area: 12
    ff(IQ, IQN) {
      clocked_on: C;
      next_state: D;
    }
    pin(C) {
      direction: input;
      clock: true;
    }
    pin(D) {
      direction: input;
    }
    pin(Q) {
      function: "IQ";
      direction: output;
    }
  }

}